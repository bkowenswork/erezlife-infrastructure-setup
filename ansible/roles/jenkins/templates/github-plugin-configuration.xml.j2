<?xml version='1.1' encoding='UTF-8'?>
<github-plugin-configuration plugin="github">
  <configs>
    <github-server-config>
      <name>Github server</name>
      <apiUrl>https://api.github.com</apiUrl>
      <manageHooks>true</manageHooks>
      <credentialsId>jenkins-github-plain-credential</credentialsId>
      <clientCacheSize>20</clientCacheSize>
    </github-server-config>
  </configs>
</github-plugin-configuration>
